{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "LOG_LEVEL": "DEBUG",
                "TYPER_DEBUG": "1"
            },
            "args": [
                "./.local_config.yaml"
            ]
        }
    ]
}
